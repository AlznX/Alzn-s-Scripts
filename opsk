BattleAxe.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
BattleAxe.BorderColor3 = Color3.fromRGB(32, 32, 32)
BattleAxe.BorderSizePixel = 2
BattleAxe.Position = UDim2.new(0.5, 0, 0.0250000004, 0)
BattleAxe.Size = UDim2.new(1, 0, 0.0500000007, -5)
BattleAxe.ZIndex = 2
BattleAxe.Font = Enum.Font.SourceSans
BattleAxe.Text = "Battle Axe"
BattleAxe.TextColor3 = Color3.fromRGB(204, 204, 204)
BattleAxe.TextScaled = true
BattleAxe.TextSize = 14.000
BattleAxe.TextWrapped = true
BattleAxe.MouseButton1Down:connect(function()
	local player = game.Players.LocalPlayer
	local character = player.Character
	local backpack = player.Backpack
	local swords = {}
	function look(dir)
		for i, v in pairs(dir:GetChildren()) do
			if(v:IsA("Tool"))then
				table.insert(swords,v)
			end
		end
	end
	look(backpack)
	look(character)

	for i = 1, math.floor(#swords/2)-1 do
		local sword = swords[i]
		sword.Parent = player.Backpack
		sword.Grip = CFrame.new(0,1.2,0)*CFrame.Angles((1.4/(#swords/2)*i)+0.8,0,0)*CFrame.new(0,-5.7,0)
		sword.Parent = player.Character
	end
	for i = math.floor(#swords/2), #swords-1 do
		local sword = swords[i]
		sword.Parent = player.Backpack
		sword.Grip = CFrame.new(0,1.2,0)*CFrame.Angles(1.4/(#swords/2)*(i-math.floor(#swords/2))+4.06,0,0)*CFrame.new(0,-5.7,0)
		sword.Parent = player.Character
	end
	local last = swords[#swords]
	last.Parent = player.Backpack
	last.Grip = CFrame.new(0,0,0)
	last.Parent = player.Character
end)

