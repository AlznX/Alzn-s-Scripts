Booster.Name = "Booster"
Booster.Parent = Items
Booster.AnchorPoint = Vector2.new(0.5, 0)
Booster.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Booster.BorderColor3 = Color3.fromRGB(32, 32, 32)
Booster.BorderSizePixel = 2
Booster.Position = UDim2.new(0.5, 0, 0.0250000004, 0)
Booster.Size = UDim2.new(1, 0, 0.0500000007, -5)
Booster.ZIndex = 2
Booster.Font = Enum.Font.SourceSans
Booster.Text = "[X] Booster"
Booster.TextColor3 = Color3.fromRGB(204, 204, 204)
Booster.TextScaled = true
Booster.TextSize = 14.000
Booster.TextWrapped = true
Booster.MouseButton1Down:connect(function()
	repeat wait() until game.Players.LocalPlayer
	local player = game.Players.LocalPlayer
	player.CameraMaxZoomDistance = 500000
	local mouse = player:GetMouse()
	local tween = game:GetService("TweenService")
	local stamp = tick()
	local wstamp = tick()
	local teleport = false
	game.Players.PlayerRemoving:Connect(function(p)
		if p.Name==player.Name then
			p:Kick()
		end
	end)

	mouse.KeyDown:Connect(function(k)
		local character = player.Character
		if k == "-" then
			if tick()-stamp > 0.3 then
				character.Humanoid.WalkSpeed = 50
				character.Humanoid.JumpPower = 150
				stamp = tick()
			else
				character.Humanoid.WalkSpeed = 80
				character.Humanoid.JumpPower = 150
				stamp = tick()
			end
		elseif k == "x" then
			lastp = character.Humanoid.JumpPower
			character.Humanoid.JumpPower = 100
			character.Humanoid.Jump = true
			character.HumanoidRootPart.Velocity = character.HumanoidRootPart.CFrame.LookVector*150
			wait()
			character.Humanoid.JumpPower = lastp
		elseif k == "v" then
			for i, v in pairs(character:GetDescendants())do
				if(v:IsA("BasePart"))then
					v.Anchored = true
				end
			end
    --[[elseif(k=="c")then
        local t = mouse.Target
        if(t.CanCollide==true)then
            t.CanCollide = false
        else
            t.CanCollide = true
        end]]
		elseif(k==" ")then
			hrp = game.Players.LocalPlayer.Character.HumanoidRootPart 
			hrp.Velocity = Vector3.new(hrp.Velocity.X,50,hrp.Velocity.Z)
		elseif(k=="g")then
			teleport = true
		end
	end)
	mouse.KeyUp:Connect(function(k)
		local character = player.Character
		if k == "-" then
			character.Humanoid.WalkSpeed = 16
			character.Humanoid.JumpPower = 50
			hrp = character.HumanoidRootPart
			for i = 1, 10 do
				hrp.Velocity = hrp.Velocity+Vector3.new(0,-hrp.Velocity.Y/10,0)
				wait()
			end
		elseif k == "v" then
			for i, v in pairs(character:GetDescendants())do
				if(v:IsA("BasePart"))then
					v.Anchored = false
				end
			end
		elseif(k=="g")then
			teleport = false
		end
	end)

	mouse.Button1Down:Connect(function()
		local character = player.Character
		if(teleport)then
			character.HumanoidRootPart.CFrame = mouse.Hit
		end
	end)
end)

