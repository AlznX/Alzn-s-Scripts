SpinningRing.Name = "SpinningRing"
SpinningRing.Parent = Items
SpinningRing.AnchorPoint = Vector2.new(0.5, 0)
SpinningRing.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
SpinningRing.BorderColor3 = Color3.fromRGB(32, 32, 32)
SpinningRing.BorderSizePixel = 2
SpinningRing.Position = UDim2.new(0.5, 0, 0.0250000004, 0)
SpinningRing.Size = UDim2.new(1, 0, 0.0500000007, -5)
SpinningRing.ZIndex = 2
SpinningRing.Font = Enum.Font.SourceSans
SpinningRing.Text = "[X] SpinningRing"
SpinningRing.TextColor3 = Color3.fromRGB(204, 204, 204)
SpinningRing.TextScaled = true
SpinningRing.TextSize = 14.000
SpinningRing.TextWrapped = true
SpinningRing.MouseButton1Down:connect(function()
	local player = game.Players.LocalPlayer
	local distance = 15
	local speed = 1
	local mouse = player:GetMouse()
	local rotating = true
	local character = player.Character
	local backpack = player.Backpack
	local swords = {}
	function look(dir)
		for i, v in pairs(dir:GetChildren()) do
			if(v:IsA("Tool"))then
				table.insert(swords,v)
			end
		end
	end
	look(backpack)
	look(character)

	mouse.KeyDown:Connect(function(k)
		if(k=="x")then
			rotating = false
		end
	end)

	for i, v in pairs(swords)do
		local sword = v
		sword.Parent = backpack
		sword.Handle.Massless = true
		sword.Handle.CanCollide = true
		sword.Grip = CFrame.new(0, 0, 0, 1, 0, 0, 0, 1, 0, 0, 0, 1)

		sword.Grip = (sword.Grip*CFrame.Angles(0,(6.28/#swords)*i,0)*CFrame.new(distance*math.sin((6.28/#swords)*i),0,distance*math.cos((6.28/#swords)*i)))
		sword.Parent = character
	end

	local counter = 0
	game:GetService("RunService").RenderStepped:Connect(function()
		if(rotating)then
			for i, v in pairs(swords)do
				counter = counter + 0.001*speed
				local sword = v
				sword.Parent = backpack
				sword.Handle.Massless = true
				sword.Handle.CanCollide = true
				sword.Grip = CFrame.new(0, 0, 0, 1, 0, 0, 0, 1, 0, 0, 0, 1)
				local angle = ((6.28/#swords)*i)+counter
				sword.Grip = (sword.Grip*CFrame.Angles(0,angle,0)*CFrame.new(distance*math.sin(angle),0,distance*math.cos(angle)))
				sword.Parent = character
			end
		end
	end)
end)

