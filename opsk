	local script = Instance.new('LocalScript', AutoFarms_2)

	local searchBar = script.Parent.SearchBar
	local items = script.Parent.Items

	function UpdateResults()
		local search = string.lower(searchBar.Text)
		for i, v in	 pairs(items:GetChildren()) do
			if v:IsA("GuiButton") then
				if search ~= "" then
					local item = string.lower(v.Text)
					if string.find(item, search) then
						v.Visible = true
					else
						v.Visible = false
					end
				else
					v.Visible = true
				end
			end
		end
	end

	searchBar.Changed:Connect(UpdateResults)
end
coroutine.wrap(CNQF_fake_script)()
local function UVLBA_fake_script() -- _2TAKE1.Notif 
	local script = Instance.new('LocalScript', _2TAKE1)

	local function callback(Text)
		if Text == "Button1 text" then
			print ("Answer")
		elseif Text == ("Button2 text") then
			print ("Answer2")
		end
	end

	local NotificationBindable = Instance.new("BindableFunction")
	NotificationBindable.OnInvoke = callback
	--
	game.StarterGui:SetCore("SendNotification",  {
		Title = "Thanks for buying Alzn#0409";
		Text = "This Menu is undetectable, we update everyday, Credits: Alzn, Have Fun!";
		Icon = "";
		Duration = 5;
		Button1 = "Close";
		Button2 = "Bye";
		Callback = NotificationBindable;
	})
end
coroutine.wrap(UVLBA_fake_script)()
local function YBQT_fake_script() -- Part.Script 
	local script = Instance.new('Script', Part)


	function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end

	counter = 0

	while wait(0.1)do
		script.Parent.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)

		counter = counter + 0.01
	end

end
coroutine.wrap(YBQT_fake_script)()
