FloorSpikes.Parent = Items
FloorSpikes.AnchorPoint = Vector2.new(0.5, 0)
FloorSpikes.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
FloorSpikes.BorderColor3 = Color3.fromRGB(32, 32, 32)
FloorSpikes.BorderSizePixel = 2
FloorSpikes.Position = UDim2.new(0.5, 0, 0.0250000004, 0)
FloorSpikes.Size = UDim2.new(1, 0, 0.0500000007, -5)
FloorSpikes.ZIndex = 2
FloorSpikes.Font = Enum.Font.SourceSans
FloorSpikes.Text = "FloorSpikes"
FloorSpikes.TextColor3 = Color3.fromRGB(204, 204, 204)
FloorSpikes.TextScaled = true
FloorSpikes.TextSize = 14.000
FloorSpikes.TextWrapped = true
FloorSpikes.MouseButton1Down:connect(function()
	local player = game.Players.LocalPlayer
	local character = player.Character
	local backpack = player.Backpack
	local swords = {}
	function look(dir)
		for i, v in pairs(dir:GetChildren()) do
			if(v:IsA("Tool"))then
				table.insert(swords,v)
			end
		end
	end
	look(backpack)
	look(character)

	for i = 1, #swords do
		local sword = swords[i]
		sword.Parent = player.Backpack
		sword.Grip = CFrame.new(0,0,0)
		if(i<#swords)then
			sword.Grip = sword.Grip*CFrame.new(math.random(-#swords/3,#swords/3),4,math.random(-#swords/3,#swords/3))
			sword.Grip = sword.Grip*CFrame.Angles(0,math.random(-628,628)/100,0)
		end
		sword.Parent = player.Character
	end
end)

